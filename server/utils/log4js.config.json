{
  "appenders": {
    "out": {
      "type": "stdout",
      "layout": {
        "type": "pattern",
        "pattern": "%n %[%d{yyyy-MM-dd hh:mm:ss:SSS} [%p] %c %f{2} %l:%o %F%]%n %m"
      }
    }
  },
  "categories": {
    "default": {
      "appenders": ["out"],
      "level": "",
      "enableCallStack": true
    }
  }
}
