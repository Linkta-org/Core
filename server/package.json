{
  "name": "linkta",
  "version": "1.0.0",
  "description": "A learning tool for people to learn fast and easy",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx server.ts",
    "build": "tsc -p tsconfig.json",
    "postbuild": "node ./utils/path-alias.js",
    "start": "node dist/server.js",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "test": "npm run test:unit && npm run test:integration",
    "test:watch": "npm run test:unit:watch & npm run test:integration:watch",
    "test:unit": "vitest run --mode unit",
    "test:unit:watch": "vitest watch --mode unit",
    "test:integration": "vitest run --mode integration",
    "test:integration:watch": "vitest watch --mode integration"
  },
  "keywords": [
    "education"
  ],
  "author": "linkta",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.1.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.26",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^5.1.3",
    "mongodb-memory-server": "^10.1.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.15.4",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.1.1",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "@types/archiver": "^6.0.2",
    "@types/node-cron": "^3.0.11",
    "archiver": "^7.0.1",
    "axios": "^1.6.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "firebase-admin": "^12.2.0",
    "log4js": "^6.9.1",
    "mongodb": "^6.4.0",
    "mongoose": "^8.2.1",
    "node": "^21.7.1",
    "node-cron": "^3.0.3",
    "reactflow": "^11.11.3",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  }
}
