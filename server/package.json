{
  "name": "linkta",
  "version": "1.0.0",
  "description": "A learning tool for people to learn fast and easy",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev1": "tsx ./server.ts",
    "dev:frontend": "vite --config client/vite.config.js",
    "dev:backend": "nodemon server.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build --config client/vite.config.js",
    "build:backend": "tsc -p tsconfig.json",
    "start": "NODE_ENV=production concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "NODE_ENV=production vite preview --config client/vite.config.js",
    "start:backend": "NODE_ENV=production node dist/server/server.ts",
    "lint": "eslint . --fix && prettier --write ."
  },
  "keywords": [
    "education"
  ],
  "author": "linkta",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.15.4",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.1.1",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "axios": "^1.6.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "log4js": "^6.9.1",
    "mongodb": "^6.4.0",
    "mongoose": "^8.2.1",
    "node": "^21.7.1",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  }
}
